<div id="<%= dom_id text_editor %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= text_editor.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Original image:</strong>
    <%= link_to text_editor.original_image.filename, text_editor.original_image, class: "text-gray-700 underline hover:no-underline" if text_editor.original_image.attached? %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Result image:</strong>
    <%= link_to text_editor.result_image.filename, text_editor.result_image, class: "text-gray-700 underline hover:no-underline" if text_editor.result_image.attached? %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Prompt:</strong>
    <%= text_editor.prompt %>
  </div>
</div>
